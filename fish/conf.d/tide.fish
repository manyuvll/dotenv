# Background color for the Git section of the prompt
set -g tide_git_bg_color 268bd2

# Background color for the Git section when in an unstable state
set -g tide_git_bg_color_unstable C4A000

# Background color for the Git section when in an urgent state
set -g tide_git_bg_color_urgent CC0000

# Color of the Git branch name in the prompt
set -g tide_git_branch_color 000000

# Color for the Git branch in the prompt
set -g tide_git_color_branch 000000

# Color for conflicted files in Git
set -g tide_git_color_conflicted 000000

# Color for dirty (modified but not staged) files in Git
set -g tide_git_color_dirty 000000

# Color for Git operations in progress (e.g., rebase, merge)
set -g tide_git_color_operation 000000

# Color for staged files in Git
set -g tide_git_color_staged 000000

# Color for stashed changes in Git
set -g tide_git_color_stash 000000

# Color for untracked files in Git
set -g tide_git_color_untracked 000000

# Color for upstream branch status in Git
set -g tide_git_color_upstream 000000

# Color for conflicted files in Git (same as tide_git_color_conflicted)
set -g tide_git_conflicted_color 000000

# Color for dirty files in Git (same as tide_git_color_dirty)
set -g tide_git_dirty_color 000000

# Icon used to represent the Git status in the prompt
set -g tide_git_icon î‚ 

# Color for Git operations in progress (same as tide_git_color_operation)
set -g tide_git_operation_color 000000

# Color for staged files in Git (same as tide_git_color_staged)
set -g tide_git_staged_color 000000

# Color for stashed changes in Git (same as tide_git_color_stash)
set -g tide_git_stash_color 000000

# Color for untracked files in Git (same as tide_git_color_untracked)
set -g tide_git_untracked_color 000000

# Color for upstream branch status in Git (same as tide_git_color_upstream)
set -g tide_git_upstream_color 000000

# Background color for the current working directory section of the prompt
set -g tide_pwd_bg_color 444444
